SELECT DISTINCT s.SCHEMA_NAME AS database_name
FROM INFORMATION_SCHEMA.SCHEMATA s
JOIN INFORMATION_SCHEMA.SCHEMATA_PRIVILEGES p
ON s.SCHEMA_NAME = p.SCHEMA_NAME
WHERE p.GRANTEE_NAME = 'your_username';
